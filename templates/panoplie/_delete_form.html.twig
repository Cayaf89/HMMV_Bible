<form method="post" action="{{ path('panoplie_delete', {'id': panoplie.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ panoplie.id) }}">
    <button class="btn">Delete</button>
</form>