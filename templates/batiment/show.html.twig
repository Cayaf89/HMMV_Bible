{% extends 'base.html.twig' %}

{% block title %}Batiment{% endblock %}

{% block body %}
    <h1>Batiment</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Nom</th>
                <td>{{ batiment.nom }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ batiment.description }}</td>
            </tr>
            <tr>
                <th>NiveauVille</th>
                <td>{{ batiment.niveauVille }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ batiment.id }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('batiment_index') }}">back to list</a>

    <a href="{{ path('batiment_edit', {'id': batiment.id}) }}">edit</a>

    {{ include('batiment/_delete_form.html.twig') }}
