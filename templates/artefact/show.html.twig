{% extends 'base_back.html.twig' %}

{% block title %}Artefact{% endblock %}

{% block body %}
    <h1>Artefact</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Nom</th>
                <td>{{ artefact.nom }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ artefact.description }}</td>
            </tr>
            <tr>
                <th>Emplacement</th>
                <td>{{ artefact.emplacement }}</td>
            </tr>
            <tr>
                <th>Importance</th>
                <td>{{ artefact.importance }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ artefact.id }}</td>
            </tr>
        </tbody>
    </table>

{% endblock %}